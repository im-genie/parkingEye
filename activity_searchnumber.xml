<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_searchnumber"
    android:background="#161713"
    tools:context=".SearchNumberActivity">


    <ImageView
        android:id="@+id/homelogo"
        android:layout_width="42dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="25dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/homelogo" />

    <ImageView
        android:id="@+id/searchnumber"
        android:layout_width="152dp"
        android:layout_height="36dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/searchnumber" />


    <ImageView
        android:id="@+id/searchnumber1"
        android:layout_width="314dp"
        android:layout_height="53dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="101dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/searchnumber1" />

    <ImageView
        android:id="@+id/numbersearchtext"
        android:layout_width="264dp"
        android:layout_height="71dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="510dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/numbersearchtext" />

    <ImageView
        android:id="@+id/yes"
        android:layout_width="114dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="594dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/yes" />

    <ImageView
        android:id="@+id/no"
        android:layout_width="114dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="238dp"
        android:layout_marginTop="594dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/no" />

    <ImageView
        android:id="@+id/button"
        android:layout_width="309dp"
        android:layout_height="57dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="721dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/button" />

    <ImageView
        android:id="@+id/state"
        android:layout_width="393dp"
        android:layout_height="62dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="650dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/state" />

    <ImageView
        android:id="@+id/carimage"
        android:layout_width="320dp"
        android:layout_height="320dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="181dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:src="@drawable/carimage" />

</RelativeLayout>